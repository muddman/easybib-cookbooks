<%
if @enabled
  @config.each do |location, conf|
-%>
    location ~* \.(<%= location %>)$ {
        expires <%= conf['expires'] %>;

        access_log <%= @access_log %>;
<%
    unless conf['headers'].nil?
      conf['headers'].each do |header|
-%>
        <%= "add_header #{header}" %>;
<%
      end
    end
%>
    }
<%
  end
end
-%>
